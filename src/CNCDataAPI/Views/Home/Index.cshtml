                    <div style="border: 1px solid #555">
                        <p style="color: #41baff">[ApiAuthorize]</p>
                        <p style="color: #41baff">[EnableCors(origins:"*", headers:"*", methods:"*")]</p>
                        <p><span style="color: #0000ff">public class</span> <span style="color: #41baff">AlignBallBrgsController : ApiController</span></p>
                        <p>{</p>
                        <p>&nbsp;&nbsp;&nbsp;&nbsp; <span style="color: #41baff">[AllowAnonymous]</span></p>
                        <p>&nbsp;&nbsp;&nbsp;&nbsp; 
                            <span style="color: #0000ff">public</span>&nbsp;
                            <span style="color: #41baff">IQueryable</span>&lt;&nbsp;
                            <span style="color: #41baff">AlignBallBrg</span>&gt; GetAlignBallBearings() 
                            <span style="color: #008000">//使用HTTP.GET请求url时会调用此方法，允许匿名调用</span>
                        </p>
                        <p>&nbsp;&nbsp;&nbsp;&nbsp; <span style="color: #41baff">[AllowAnonymous]</span></p>
                        <p>&nbsp;&nbsp;&nbsp;&nbsp; 
                            <span style="color: #0000ff">public async</span>&nbsp;
                            <span style="color: #41baff">Task</span>>&lt;
                            <span style="color: #41baff">IHttpActionResult</span>&gt; GetAlignBallBrg(<span style="color: #0000ff">string</span> id) 
                            <span style="color: #008000">//使用HTTP.GET请求url并且附有id参数时会调用此方法，允许匿名调用</span>
                        </p>
                        <p>&nbsp;&nbsp;&nbsp;&nbsp; 
                            <span style="color: #0000ff">public async</span>&nbsp;
                            <span style="color: #41baff">Task</span>&lt;
                            <span style="color: #41baff">IHttpActionResult</span>&gt; PutAlignBallBrg(<span style="color: #0000ff">string</span> id, <span style="color: #41baff">AlignBallBrg</span> alignBallBrg) 
                            <span style="color: #008000">//使用HTTP.PUT请求url时会调用此方法，不允许匿名调用</span>
                        </p>
                        <p>&nbsp;&nbsp;&nbsp;&nbsp; 
                            <span style="color: #0000ff">public async</span>&nbsp;
                            <span style="color: #41baff">Task</span>&lt;
                            <span style="color: #41baff">IHttpActionResult</span>&gt; PostAlignBallBrg(<span style="color: #41baff">AlignBallBrg</span> alignBallBrg) 
                            <span style="color: #008000">//使用HTTP.POST请求url时会调用此方法，不允许匿名调用</span>
                        </p>
                        <p>&nbsp;&nbsp;&nbsp;&nbsp; 
                            <span style="color: #0000ff">public async</span>&nbsp;
                            <span style="color: #41baff">Task</span>&lt;
                            <span style="color: #41baff">IHttpActionResult</span>&gt; DeleteAlignBallBrg(<span style="color: #0000ff">string</span> id) 
                            <span style="color: #008000">//使用HTTP.DELETE请求url时会调用此方法，不允许匿名调用</span>
                        </p>
